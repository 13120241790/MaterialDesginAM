<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.materialdesignam">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name="com.materialdesignam.activity.HomeActivity"></activity>
        <activity android:name="com.materialdesignam.activity.OtherActivity"></activity>
        <activity android:name="com.materialdesignam.activity.DialogPlusActivity"></activity>
        <activity android:name="com.materialdesignam.activity.NewtonCradleActivity"></activity>
        <activity android:name="com.materialdesignam.activity.RotateActivity"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
